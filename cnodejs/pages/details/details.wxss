/**detils.wxss**/
.box{
  width: 92%;
  margin: 0 auto;
  font-family: 微软雅黑;
}
.box .tit_box{
  color: #80BD01;
  padding: 15rpx 0;
  font-size: 1.2rem;
}
.box .con_box{
  font-size: .8rem;
  color: #666;
  word-break: break-all;
  line-height: 1.3rem;
  color: #777;
}
.img{
  width:100%;
  height: auto;
  display: block;
}
.huifu_tit{
  width: 100%;
  height: 66rpx;
  background: #e9e9e9;
  font-size: 0.8rem;
  padding: 8rpx 10rpx;
  line-height: 50rpx;
  box-sizing: border-box;
  color: #777;
}
.huifu_tit image{
  width:50rpx;
  height: 50rpx;
  
}
.huifu_con{
  margin:25rpx 0;
  font-size: 0.8rem;
  word-break: break-all;
}
/* loading */
.load{
  width: 140rpx;
  height: 140rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  margin-top: -70rpx;
  margin-left: -70rpx;
  background: #000; 
  opacity: 0.7;
  border-radius: 8rpx;
  color: #fff;
  line-height: 140rpx;
  text-align: center;
  z-index: 1000;
  font-size: 0.8rem;

}
.mask{
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.5;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
}
.fr{
  float: right;
}
.fl{
  float: left;
}